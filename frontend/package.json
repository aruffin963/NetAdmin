    "@vitest/ui": "^4.0.6",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
