    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/d3": "^7.4.3",
    "axios": "^1.6.2",
    "d3": "^7.9.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.29.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
